{
  "$schema": "https://developer.microsoft.com/json-schemas/tsdoc/v0/tsdoc.schema.json",
  "supportForTags": {
    "@example": true,
    "@link": true,
    "@param": true,
    "@remarks": true,
    "@returns": true,
    "@see": true,
    "@since": true,
    "@TODO": true,
    "@typeParam": true,
    "@deprecated": true
  },
  "tagDefinitions": [
    {
      "tagName": "@since",
      "syntaxKind": "block",
      "allowMultiple": false
    },
    {
      "tagName": "@TODO",
      "syntaxKind": "block"
    }
  ]
}
